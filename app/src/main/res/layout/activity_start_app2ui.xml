<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".start_app_2ui">

    <ImageView
        android:id="@+id/imageView29"
        android:layout_width="450dp"
        android:layout_height="350dp"
        android:layout_marginTop="-50dp"
        android:layout_marginBottom="32dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.508"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.03"
        app:srcCompat="@drawable/start_ui_background" />

    <Button
        android:id="@+id/button11"
        android:layout_width="283dp"
        android:layout_height="72dp"
        android:layout_marginTop="224dp"
        android:background="@drawable/button_gradient"
        android:bottomLeftRadius="200dp"
        android:bottomRightRadius="200dp"
        android:text="2023 Advance Level"
        android:textColor="#FDFDFD"
        android:textSize="24sp"
        android:textStyle="bold"
        app:backgroundTint="@null"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button12"
        android:layout_width="337dp"
        android:layout_height="75dp"
        android:layout_marginTop="28dp"
        android:background="@drawable/button_gradient"
        android:bottomLeftRadius="200dp"
        android:bottomRightRadius="200dp"
        android:text="Art Stream"
        android:textColor="#FDFDFD"
        android:textSize="24sp"
        app:backgroundTint="@null"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView29" />

    <Button
        android:id="@+id/button13"
        android:layout_width="337dp"
        android:layout_height="75dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/button_gradient"
        android:bottomLeftRadius="200dp"
        android:bottomRightRadius="200dp"
        android:text="Commerce Stream"
        android:textColor="#FDFDFD"
        android:textSize="24sp"
        app:backgroundTint="@null"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button12" />

    <Button
        android:id="@+id/button14"
        android:layout_width="337dp"
        android:layout_height="75dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/button_gradient"
        android:bottomLeftRadius="200dp"
        android:bottomRightRadius="200dp"
        android:text="Science Stream"
        android:textColor="#FDFDFD"
        android:textSize="24sp"
        app:backgroundTint="@null"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button13" />

    <Button
        android:id="@+id/button15"
        android:layout_width="337dp"
        android:layout_height="75dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/button_gradient"
        android:bottomLeftRadius="200dp"
        android:bottomRightRadius="200dp"
        android:text="Technology Stream"
        android:textColor="#FDFDFD"
        android:textSize="24sp"
        app:backgroundTint="@null"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button14" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:text="2023 Advace Level"
        android:textColor="#FBF9F9"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/button11"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.59" />

    <ImageView
        android:id="@+id/imageView30"
        android:layout_width="50dp"
        android:layout_height="38dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.952"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/whitecolor_notification" />

</androidx.constraintlayout.widget.ConstraintLayout>